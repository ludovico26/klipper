# Test case for z_tilt and quad_gantry_level
CONFIG z_tilt.cfg
DICTIONARY atmega2560.dict

# Start by homing the printer.
G28
G1 Z5 X10 Y10 F6000
M400
GET_POSITION

# Run Z_TILT_ADJUST in manual mode
Z_TILT_ADJUST METHOD=MANUAL
G1 Z2.909972
ACCEPT
G1 Z2.924972
ACCEPT
G1 Z2.959972
ACCEPT
G1 Z2.924972
ACCEPT

# Report position
G1 Z5 X10 Y10 F6000
M400
GET_POSITION

# Run again in automatic mode
Z_TILT_ADJUST

#MY CHANGES EXAMPLE..
G1 Z25 F3000
MODIFY_ROTATION A=0.01 B=0.02 C=0.03 D=0.04 SPEED=4
